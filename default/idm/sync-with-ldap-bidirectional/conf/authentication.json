                    "queryOnResource" : "system/ldap/account",
                    "propertyMapping" : {
                        "authenticationId" : "uid"
                    },
                    "managedUserLink" : "systemLdapAccounts_managedUser",
                    "defaultUserRoles" : [
                        "openidm-authorized"
                    ]
                },
                "enabled" : true
            },
            {
                "name" : "SOCIAL_PROVIDERS",
                "properties" : {
                    "defaultUserRoles" : [
                        "openidm-authorized"
                    ],
                    "augmentSecurityContext" : {
                        "type" : "text/javascript",
                        "globals" : { },
                        "file" : "auth/populateAsManagedUserFromRelationship.js"
                    },
                    "propertyMapping" : {
                        "userRoles" : "authzRoles"
                    }
                },
                "enabled" : true
            },
            {
                "name" : "OAUTH_CLIENT",
                "properties" : {
                    "augmentSecurityContext" : {
                        "type" : "text/javascript",
                        "globals" : {
                            "sessionValidationBaseEndpoint" : "http://openam/openam/json/sessions/"
